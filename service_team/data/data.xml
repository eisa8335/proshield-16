<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_service_contract" model="ir.sequence">
            <field name="name">Contract sequence</field>
            <field name="code">service.contract</field>
            <field name="prefix">CPS/%(year)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="sequence_service_quotation" model="ir.sequence">
            <field name="name">Quotation sequence</field>
            <field name="code">service.quotation</field>
            <field name="prefix">QPS/%(year)s/</field>
            <field name="padding">5</field>
        </record>
        <record id="sequence_job_event" model="ir.sequence">
            <field name="name">job event sequence</field>
            <field name="code">job.event</field>
            <field name="prefix">JOB/</field>
        </record>
        <record id="channel_contract" model="mail.channel">
            <field name="name">Contracts</field>
        </record>
        <record id="channel_quotation" model="mail.channel">
            <field name="name">Quotations</field>
        </record>
        <record id="channel_job" model="mail.channel">
            <field name="name">Jobs</field>
        </record>
        <record id="job_type_contract" model="job.type">
            <field name="name">Contract</field>
        </record>

    </data>
</odoo>